{
  "name": "prisma-template",
  "repository": {
    "url": "https://github.com/2wce/prisma-template"
  },
  "author": {
    "name": "Kudakwashe Terrence Mupeni",
    "url": "http://2wce.github.io",
    "email": "kudamupeni@icloud.com"
  },
  "license": "MIT",
  "scripts": {
    "prestart": "graphql-codegen && npm run generate && npm run reset",
    "start": "ts-node-dev --respawn --transpile-only src",
    "reset": "npx prisma migrate reset -f",
    "seed": "ts-node prisma/seed.ts",
    "generate": "prisma generate",
    "studio": "prisma studio",
    "migrate": "prisma migrate dev",
    "postmigrate": "yarn generate",
    "pretest": "graphql-codegen",
    "test": "jest",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "dependencies": {
    "@graphql-tools/schema": "^8.2.0",
    "@prisma/client": "3.3.0",
    "apollo-server": "2.25.2",
    "bcryptjs": "2.4.3",
    "factory.ts": "^0.5.2",
    "faker": "^5.5.3",
    "graphql": "15.6.1",
    "graphql-middleware": "^6.1.8",
    "graphql-scalars": "1.10.1",
    "graphql-shield": "^7.5.0",
    "graphql-tools": "^8.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "nodemailer": "^6.7.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.0",
    "@graphql-codegen/typescript": "2.2.4",
    "@graphql-codegen/typescript-resolvers": "2.3.2",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/commit-analyzer": "^9.0.1",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/github": "^8.0.1",
    "@semantic-release/npm": "^8.0.0",
    "@semantic-release/release-notes-generator": "^10.0.2",
    "@types/bcryptjs": "2.4.2",
    "@types/faker": "^5.5.8",
    "@types/jest": "27.0.2",
    "@types/lodash": "4.14.175",
    "@types/node": "14.17.27",
    "@types/ws": "8.2.0",
    "ansi-regex": "6.0.1",
    "jest": "^27.2.5",
    "jest-mock-extended": "^2.0.4",
    "prisma": "^3.3.0",
    "semantic-release": "^18.0.0",
    "string-width": "5.0.1",
    "strip-ansi": "6.0.1",
    "ts-jest": "^27.0.6",
    "ts-node": "10.3.0",
    "ts-node-dev": "1.1.8",
    "typescript": "4.4.4"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "engines": {
    "node": ">=14.17"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "resolutions": {
    "ansi-regex": "6.0.1",
    "strip-ansi": "6.0.1",
    "string-width": "5.0.1",
    "ws": "7.4.6"
  }
}
